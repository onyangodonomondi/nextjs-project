{
  "name": "mocky-digital",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "DEBUG=* next dev",
    "dev:nextjs": "DEBUG=next:* next dev",
    "dev:trace": "NODE_OPTIONS='--trace-deprecation' next dev",
    "dev:rendering": "DEBUG=next:rendering* next dev",
    "dev:router": "DEBUG=next:router* next dev",
    "dev:build": "DEBUG=next:build* next dev",
    "dev:full": "DEBUG=next:*,react:* next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "optimize-images": "node scripts/optimize-images.js",
    "clean": "rimraf .next node_modules/.cache",
    "reset": "rimraf .next node_modules package-lock.json && npm install",
    "dev:clean": "npm run clean && npm run dev",
    "build:prod": "NODE_ENV=production next build",
    "dev:fast": "NEXT_MINIMAL=1 NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:memory": "NODE_OPTIONS='--max-old-space-size=8192' next dev"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@tanstack/react-virtual": "^3.13.4",
    "aos": "^2.3.4",
    "browser-image-compression": "^2.0.2",
    "clsx": "^2.1.1",
    "framer-motion": "10.18.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "next": "^15.2.2",
    "p-queue": "^8.1.0",
    "plaiceholder": "^3.0.0",
    "react": "18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-quill": "^2.0.0",
    "react-type-animation": "^3.2.0",
    "react-window": "^1.8.11",
    "slugify": "^1.6.6",
    "swiper": "^11.2.5"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/lodash": "^4.17.16",
    "@types/mime-types": "^2.1.4",
    "@types/node": "20.17.24",
    "@types/react": "18.3.18",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "cacheable-request": "^7.0.4",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.2",
    "glob": "^10.3.10",
    "got": "^12.6.1",
    "http-cache-semantics": "^4.1.1",
    "imagemin": "^9.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "postcss": "^8.5.3",
    "rimraf": "^5.0.5",
    "semver-regex": "^4.0.5",
    "sharp": "^0.33.5",
    "squoosh": "^0.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.2",
    "uuid": "^9.0.1"
  }
}
